<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RipplePulseRelativeLayout">
        <attr name="pulse_layout_ShowPreview" format="boolean" />
        <attr name="pulse_layout_RippleColor" format="color|reference" />
        <attr name="pulse_layout_PulseType" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
        </attr>
        <attr name="pulse_layout_PulseDuration" format="integer|reference" />
        <attr name="pulse_layout_StartDelay" format="integer|reference" />
        <attr name="pulse_layout_EndDelay" format="integer|reference" />
        <attr name="pulse_layout_RippleStrokeWidth" format="dimension|reference" />
        <attr name="pulse_layout_RippleStartRadiusPercent" format="float" />
        <attr name="pulse_layout_RippleEndRadiusPercent" format="float" />
        <attr name="pulse_layout_PulseInterpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="IncreaseDecreaseButton">
        <attr name="initNumber" format="integer" />
        <attr name="minNumber" format="integer" />
        <attr name="maxNumber" format="integer" />
        <attr name="activeIconColor" format="color" />
        <attr name="inactiveIconColor" format="color" />
        <attr name="textSizeNumber" format="dimension" />
        <attr name="iconSize" format="dimension" />
    </declare-styleable>

</resources>